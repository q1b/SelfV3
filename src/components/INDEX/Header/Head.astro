---
import HeadCommon from "./HeadCommon.astro"
import HeadSEO from "./HeadSEO.astro"
const { content ,isMarkdown=false } = Astro.props;
const defaultContent = {
    title:"Suprito",
    description:"Portfolio website..."
}
---
<head>
    <HeadCommon isMarkdown={isMarkdown} />
    <HeadSEO site="" content={ content ?? defaultContent } canonicalURL={Astro.request.canonicalURL} />
</head>